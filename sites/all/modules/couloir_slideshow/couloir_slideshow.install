/**
 * Implementation of hook_uninstall().
 */
function couloir_slideshow_uninstall() {
  foreach (node_get_types('names') as $typename => $visiblename) {
    if (variable_get('couloir_slideshow_'. $typename, 0)) {
      variable_del('couloir_slideshow_'. $typename);
    }
  }
}